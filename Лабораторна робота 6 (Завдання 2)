# Функція для пошуку підсписка в основному списку
def find_sublist(main_list, sublist):
    # Перевірка, чи є підсписок у основному списку
    n = len(main_list)
    m = len(sublist)
    
    for i in range(n - m + 1):
        if main_list[i:i + m] == sublist:  # Порівнюємо підсписок
            return i  # Повертаємо індекс першого елемента підсписка в основному списку
    
    return -1  # Якщо підсписок не знайдено, повертаємо -1

# Основна частина програми
# Користувач вводить основний список
main_input = input("Введіть основний список елементів через пробіл: ")
main_list = list(map(int, main_input.split()))  # Перетворюємо введені значення в список цілих чисел

# Користувач вводить підсписок, який потрібно знайти
sub_input = input("Введіть підсписок елементів через пробіл: ")
sublist = list(map(int, sub_input.split()))  # Перетворюємо введені значення в підсписок

# Викликаємо функцію для пошуку підсписка
index = find_sublist(main_list, sublist)

# Виводимо результат
if index != -1:
    print(f"Підсписок знайдено на позиції: {index}")
else:
    print("Підсписок не знайдений.")
